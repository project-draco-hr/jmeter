{
  Iterator iter=testListeners.getSearchResults().iterator();
  while (iter.hasNext()) {
    TestListener tl=(TestListener)iter.next();
    if (tl instanceof TestBean)     TestBeanHelper.prepare((TestElement)tl);
    if (host == null) {
      tl.testStarted();
    }
 else {
      tl.testStarted(host);
    }
  }
}
