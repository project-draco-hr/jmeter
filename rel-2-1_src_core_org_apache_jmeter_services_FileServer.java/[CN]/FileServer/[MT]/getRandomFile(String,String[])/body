{
  File input=null;
  if (basedir != null) {
    File src=new File(basedir);
    if (src.isDirectory() && src.list() != null) {
      File[] lfiles=src.listFiles(new JMeterFileFilter(extensions));
      int count=lfiles.length;
      input=lfiles[random.nextInt(count)];
    }
  }
  return input;
}
