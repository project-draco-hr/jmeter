{
  try {
    return method.invoke(invokee,params);
  }
 catch (  IllegalArgumentException e) {
    log.error("This should never happen.",e);
    throw new Error(e.toString());
  }
catch (  IllegalAccessException e) {
    log.error("This should never happen.",e);
    throw new Error(e.toString());
  }
catch (  InvocationTargetException e) {
    log.error("This should never happen.",e);
    throw new Error(e.toString());
  }
}
