{
  try {
    Map newCol=(Map)value.getClass().newInstance();
    PropertyIterator iter=valueIterator();
    while (iter.hasNext()) {
      JMeterProperty item=iter.next();
      newCol.put(item.getName(),item.clone());
    }
    return newCol;
  }
 catch (  Exception e) {
    log.error("Couldn't clone map",e);
    return value;
  }
}
