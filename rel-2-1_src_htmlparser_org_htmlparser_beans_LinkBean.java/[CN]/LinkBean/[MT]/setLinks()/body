{
  String url;
  URL[] urls;
  URL[] oldValue;
  url=getURL();
  if (null != url)   try {
    urls=extractLinks(getURL());
    if (!equivalent(mLinks,urls)) {
      oldValue=mLinks;
      mLinks=urls;
      mPropertySupport.firePropertyChange(PROP_LINKS_PROPERTY,oldValue,mLinks);
    }
  }
 catch (  ParserException hpe) {
    mLinks=null;
  }
}
