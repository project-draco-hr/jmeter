{
  if (null != getURL())   try {
    mParser.flushScanners();
    mParser.registerScanners();
    mIsPre=false;
    mIsScript=false;
    try {
      mBuffer=new StringBuffer(4096);
      mParser.visitAllNodesWith(this);
      updateStrings(mBuffer.toString());
    }
  finally {
      mBuffer=null;
    }
  }
 catch (  ParserException pe) {
    updateStrings(pe.toString());
  }
}
