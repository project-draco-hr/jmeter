{
  boolean links;
  String url;
  StringExtractor se;
  links=false;
  url=null;
  for (int i=0; i < args.length; i++)   if (args[i].equalsIgnoreCase("-links"))   links=true;
 else   url=args[i];
  if (null != url) {
    se=new StringExtractor(url);
    try {
      System.out.println(se.extractStrings(links));
    }
 catch (    ParserException e) {
      e.printStackTrace();
    }
  }
 else   System.out.println("Usage: java -classpath htmlparser.jar org.htmlparser.parserapplications.StringExtractor [-links] url");
}
