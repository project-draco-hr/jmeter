{
  createParser("<%\n" + "public String getHref(String value) \n" + "{ \n"+ "int indexs = value.indexOf(\"<A HREF=\");\n"+ "int indexe = value.indexOf(\">\");\n"+ "if (indexs != -1) {\n"+ "return value.substring(indexs+9,indexe-2);\n"+ "}\n"+ "return value;\n"+ "}\n"+ "%>\n");
  Parser.setLineSeparator("\r\n");
  parser.addScanner(new JspScanner("-j"));
  parseAndAssertNodeCount(1);
}
