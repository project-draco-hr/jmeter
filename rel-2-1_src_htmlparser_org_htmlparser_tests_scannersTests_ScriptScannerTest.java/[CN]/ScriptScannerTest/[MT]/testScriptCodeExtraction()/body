{
  createParser("<SCRIPT language=JavaScript>" + "document.write(\"<a href=\"1.htm\"><img src=\"1.jpg\" " + "width=\"80\" height=\"20\" border=\"0\"></a>\");"+ "</SCRIPT>");
  parser.registerScanners();
  parseAndAssertNodeCount(1);
  assertType("script",ScriptTag.class,node[0]);
  ScriptTag scriptTag=(ScriptTag)node[0];
  assertStringEquals("script code","document.write(\"<a href=\"1.htm\"><img src=\"1.jpg\" " + "width=\"80\" height=\"20\" border=\"0\"></a>\");",scriptTag.getScriptCode());
}
