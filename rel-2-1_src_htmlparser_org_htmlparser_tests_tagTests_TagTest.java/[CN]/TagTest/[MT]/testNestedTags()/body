{
  EndTag etag;
  String s="input type=\"text\" value=\"<%=\"test\"%>\" name=\"text\"";
  String line="<" + s + ">";
  createParser(line);
  parseAndAssertNodeCount(1);
  assertTrue("The node found should have been an Tag",node[0] instanceof Tag);
  Tag tag=(Tag)node[0];
  assertEquals("Tag Contents",s,tag.getText());
}
