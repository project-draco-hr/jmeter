{
  StringBean sb;
  String text;
  byte[] data;
  sb=new StringBean();
  sb.setURL("http://htmlparser.sourceforge.net/test/example.html");
  text=sb.getStrings();
  data=pickle(sb);
  sb=(StringBean)unpickle(data);
  assertEquals("Strings before and after serialization differ",text,sb.getStrings());
}
