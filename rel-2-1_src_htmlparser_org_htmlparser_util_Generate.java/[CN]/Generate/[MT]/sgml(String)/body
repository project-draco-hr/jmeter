{
  int index;
  int begin;
  int end;
  StringBuffer ret;
  ret=new StringBuffer(4096);
  index=0;
  while (-1 != (begin=string.indexOf("<",index))) {
    if (-1 != (end=string.indexOf("-->",begin))) {
      extract(string.substring(begin,end + 3));
      index=end + 3;
    }
 else     index=begin + 1;
  }
}
