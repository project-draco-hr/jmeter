{
  HashTree subTree=propTree.getTree(keys);
  if (subTree != null) {
    if (subTree.list() == null || subTree.list().size() == 0) {
      return def;
    }
 else     if (subTree.list().size() == 1) {
      return (String)subTree.getArray()[0];
    }
 else {
      return def;
    }
  }
 else {
    return def;
  }
}
