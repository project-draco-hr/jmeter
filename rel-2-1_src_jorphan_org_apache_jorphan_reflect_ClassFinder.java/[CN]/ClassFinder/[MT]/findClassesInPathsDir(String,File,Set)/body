{
  String[] list=dir.list();
  for (int i=0; i < list.length; i++) {
    File file=new File(dir,list[i]);
    if (file.isDirectory()) {
      findClassesInPathsDir(strPathElement,file,listClasses);
    }
 else     if (list[i].endsWith(DOT_CLASS) && file.exists() && (file.length() != 0)) {
      final String path=file.getPath();
      listClasses.add(path.substring(strPathElement.length() + 1,path.lastIndexOf(".")).replace(File.separator.charAt(0),'.'));
    }
  }
}
