{
  Method[] meths=obj.getClass().getMethods();
  for (int idx=0; idx < meths.length; idx++) {
    if (meths[idx].getName().startsWith(PREFIX) || meths[idx].getName().equals(ONETIMESETUP) || meths[idx].getName().equals(ONETIMETEARDOWN)|| meths[idx].getName().equals(SUITE)) {
      list.add(meths[idx]);
    }
  }
  if (list.size() > 0) {
    Method[] rmeth=new Method[list.size()];
    return (Method[])list.toArray(rmeth);
  }
  return new Method[0];
}
