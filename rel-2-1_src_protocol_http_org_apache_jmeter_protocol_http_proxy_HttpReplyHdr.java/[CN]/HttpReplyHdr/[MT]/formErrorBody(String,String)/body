{
  StringBuffer out=new StringBuffer();
  out.append("<HTML><HEAD><TITLE>");
  out.append(error);
  out.append("</TITLE></HEAD>");
  out.append("<BODY><H2>").append(error).append("</H2>\n");
  out.append("</P></H3>");
  out.append(description);
  out.append("</BODY></HTML>");
  return out.toString();
}
