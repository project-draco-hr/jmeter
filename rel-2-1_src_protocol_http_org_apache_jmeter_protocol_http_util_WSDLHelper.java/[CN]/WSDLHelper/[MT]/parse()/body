{
  try {
    this.connect();
    this.buildDocument();
    SOAPOPS=this.getOperations();
  }
 catch (  IOException exception) {
    throw (new WSDLException(exception));
  }
catch (  SAXException exception) {
    throw (new WSDLException(exception));
  }
catch (  ParserConfigurationException exception) {
    throw (new WSDLException(exception));
  }
 finally {
    this.close();
  }
}
