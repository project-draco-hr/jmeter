{
  if (!started[0]) {
    try {
      this.setRunningVersion(true);
      TestBeanHelper.prepare(this);
      initPool();
    }
 catch (    Exception e) {
      log.error("Unable to start database connection pool.",e);
    }
  }
  getThreadContext().getVariables().putObject(getDataSource(),excaliburSource);
}
