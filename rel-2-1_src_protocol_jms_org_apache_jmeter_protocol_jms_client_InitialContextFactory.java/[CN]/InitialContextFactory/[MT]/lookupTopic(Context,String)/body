{
  Topic t=null;
  if (name != null && ctx != null) {
    try {
      t=(Topic)ctx.lookup(name);
    }
 catch (    NamingException e) {
      log.error("JNDI error: " + e.getMessage());
    }
  }
 else   if (name == null) {
    log.error("lookupTopic: name was null");
  }
 else {
    log.error("lookupTopic: Context was null");
  }
  return t;
}
