{
  LOGGER.debug("Thread ended " + new Date());
  super.threadFinished();
  if (session != null)   try {
    session.close();
  }
 catch (  JMSException e) {
    LOGGER.info(e.getLocalizedMessage());
  }
  if (connection != null)   try {
    connection.close();
  }
 catch (  JMSException e) {
    LOGGER.info(e.getLocalizedMessage());
  }
  if (receiverThread != null)   receiverThread.deactivate();
}
