{
  super.clearGui();
  GuiUtils.stopTableEditing(stringTable);
  tableModel.clearData();
  responseStringButton.setSelected(true);
  urlButton.setSelected(false);
  responseCodeButton.setSelected(false);
  responseMessageButton.setSelected(false);
