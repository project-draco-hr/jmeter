{
  if (state == 1) {
    equalButton.setSelected(true);
    execState=1;
  }
 else   if (state == 2) {
    notequalButton.setSelected(true);
    execState=2;
  }
 else   if (state == 3) {
    greaterthanButton.setSelected(true);
    execState=3;
  }
 else   if (state == 4) {
    lessthanButton.setSelected(true);
    execState=4;
  }
 else   if (state == 5) {
    greaterthanequalButton.setSelected(true);
    execState=5;
  }
 else   if (state == 6) {
    lessthanequalButton.setSelected(true);
    execState=6;
  }
}
