{
  Sampler sampler=controller.next();
  if (sampler == null) {
    currentReturnedNull(controller);
    return doNext();
  }
 else {
    currentReturnedAtLeastOne=true;
    if (getStyle() == IGNORE_SUB_CONTROLLERS) {
      incrementCurrent();
      skipNext=true;
    }
 else {
      searchStart=null;
    }
    return sampler;
  }
}
