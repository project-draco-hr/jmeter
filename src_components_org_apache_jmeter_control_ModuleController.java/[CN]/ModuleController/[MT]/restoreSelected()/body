{
  if (selectedNode == null) {
    Vector nodePath=getNodePath();
    if (nodePath != null && nodePath.size() > 0) {
      GuiPackage gp=GuiPackage.getInstance();
      if (gp != null) {
        JMeterTreeNode root=(JMeterTreeNode)gp.getTreeModel().getRoot();
        nodePath.remove(0);
        traverse(root,nodePath);
      }
    }
  }
}
