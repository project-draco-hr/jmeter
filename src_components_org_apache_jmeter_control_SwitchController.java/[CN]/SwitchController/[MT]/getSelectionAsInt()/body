{
  int ret;
  getProperty(SWITCH_VALUE).recoverRunningVersion(null);
  String sel=getSelection();
  try {
    ret=Integer.parseInt(sel);
    if (ret < 0 || ret >= getSubControllers().size()) {
      ret=0;
    }
  }
 catch (  NumberFormatException e) {
    if (sel.length() == 0) {
      ret=0;
    }
 else {
      ret=scanControllerNames(sel);
    }
  }
  return ret;
}
