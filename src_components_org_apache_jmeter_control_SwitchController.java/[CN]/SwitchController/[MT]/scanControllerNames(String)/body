{
  Iterator<TestElement> iter=getSubControllers().iterator();
  int i=0;
  int default_pos=Integer.MAX_VALUE;
  while (iter.hasNext()) {
    TestElement el=iter.next();
    String name=el.getName();
    if (name.equals(sel)) {
      return i;
    }
    if (name.equalsIgnoreCase("default")) {
      default_pos=i;
    }
    i++;
  }
  return default_pos;
}
