{
  if (returnTrue)   return true;
  int executions, iterations;
  boolean retval=false;
  executions=getExecutions();
  iterations=getIteration();
  if (getStyle() == BYNUMBER) {
    if (executions < getMaxThroughput()) {
      retval=true;
    }
  }
 else {
    if (iteration == 0 && getPercentThroughput() > 0)     retval=true;
 else     if ((executions / iteration) * 100 <= getPercentThroughput())     retval=true;
  }
  if (retval) {
    returnTrue=true;
    increaseExecutions();
  }
  return retval;
}
