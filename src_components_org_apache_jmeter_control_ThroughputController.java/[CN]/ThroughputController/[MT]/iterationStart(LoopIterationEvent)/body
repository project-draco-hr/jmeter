{
  if (!isPerThread()) {
synchronized (counterLock) {
      globalIteration++;
      runThisTime=decide(globalNumExecutions,globalIteration);
      if (runThisTime)       globalNumExecutions++;
    }
  }
 else {
    iteration++;
    runThisTime=decide(numExecutions,iteration);
    if (runThisTime)     numExecutions++;
  }
}
