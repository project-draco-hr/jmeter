{
  JPopupMenu menu=new JPopupMenu();
  JMenu addMenu=MenuFactory.makeMenus(new String[]{MenuFactory.CONFIG_ELEMENTS,MenuFactory.ASSERTIONS,MenuFactory.TIMERS,MenuFactory.LISTENERS},JMeterUtils.getResString("Add"),"Add");
  menu.add(addMenu);
  MenuFactory.addEditMenu(menu,true);
  MenuFactory.addFileMenu(menu);
  return menu;
}
