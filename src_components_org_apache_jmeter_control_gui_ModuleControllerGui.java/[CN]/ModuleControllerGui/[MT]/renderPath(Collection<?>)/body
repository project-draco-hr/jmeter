{
  Iterator<?> iter=path.iterator();
  StringBuilder buf=new StringBuilder();
  boolean first=true;
  while (iter.hasNext()) {
    if (first) {
      first=false;
      iter.next();
      continue;
    }
    buf.append(iter.next());
    if (iter.hasNext()) {
      buf.append(" > ");
    }
  }
  return buf.toString();
}
