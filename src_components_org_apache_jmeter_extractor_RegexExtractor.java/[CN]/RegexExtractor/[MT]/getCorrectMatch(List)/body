{
  if (getMatchNumber() == matches.size() && matches.size() > 0) {
    return (MatchResult)matches.get(matches.size() - 1);
  }
 else   if (getMatchNumber() == 0 && matches.size() > 0) {
    return (MatchResult)matches.get(JMeterUtils.getRandomInt(matches.size()));
  }
 else {
    return null;
  }
}
