{
  String inputString=useUrl() ? result.getUrlAsString() : useHeaders() ? result.getResponseHeaders() : useCode() ? result.getResponseCode() : useMessage() ? result.getResponseMessage() : useUnescapedBody() ? StringEscapeUtils.unescapeHtml4(result.getResponseDataAsString()) : useBodyAsDocument() ? Document.getTextFromDocument(result.getResponseData()) : result.getResponseDataAsString();
  if (log.isDebugEnabled()) {
    log.debug("Input = " + inputString);
  }
  return inputString;
}
