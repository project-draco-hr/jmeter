{
  JPanel panel=new JPanel(new GridBagLayout());
  GridBagConstraints gbc=new GridBagConstraints();
  initConstraints(gbc);
  addField(panel,refNameField,gbc);
  resetContraints(gbc);
  addField(panel,xpathQueryField,gbc);
  resetContraints(gbc);
  gbc.weighty=1;
  addField(panel,defaultField,gbc);
  return panel;
}
