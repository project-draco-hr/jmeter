{
  long delay=0;
  BSFManager mgr=null;
  try {
    mgr=getManager();
    Object o=evalFileOrScript(mgr);
    if (o == null) {
      log.warn("Script did not return a value");
      return 0;
    }
    delay=Long.valueOf(o.toString()).longValue();
  }
 catch (  NumberFormatException e) {
    log.warn("Problem in BSF script " + e);
  }
catch (  BSFException e) {
    log.warn("Problem in BSF script " + e);
  }
 finally {
    if (mgr != null) {
      mgr.terminate();
    }
  }
  return delay;
}
