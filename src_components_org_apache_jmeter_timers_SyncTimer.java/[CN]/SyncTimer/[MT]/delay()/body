{
  if (getGroupSize() >= 0) {
    int arrival=0;
    try {
      arrival=this.barrier.await();
    }
 catch (    InterruptedException e) {
      return 0;
    }
catch (    BrokenBarrierException e) {
      return 0;
    }
 finally {
      if (arrival == 0) {
        barrier.reset();
      }
    }
  }
  return 0;
}
