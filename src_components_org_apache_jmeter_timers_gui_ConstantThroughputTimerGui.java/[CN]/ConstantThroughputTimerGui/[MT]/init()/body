{
  setLayout(new VerticalLayout(5,VerticalLayout.LEFT,VerticalLayout.TOP));
  setBorder(makeBorder());
  setLayout(new VerticalLayout(5,VerticalLayout.LEFT));
  add(makeTitlePanel());
  Box throughputPanel=Box.createHorizontalBox();
