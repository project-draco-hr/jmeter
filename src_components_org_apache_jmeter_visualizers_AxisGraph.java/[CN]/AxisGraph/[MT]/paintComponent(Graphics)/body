{
  if (data != null && this.title != null && this.xAxisLabels != null && this.xAxisTitle != null && this.yAxisLabel != null && this.yAxisTitle != null) {
    drawSample(this.title,this.maxLength,this.xAxisLabels,this.xAxisTitle,this.yAxisTitle,this.legendLabels,this.data,this.width,this.height,this.color,this.legendFont,graphics);
  }
}
