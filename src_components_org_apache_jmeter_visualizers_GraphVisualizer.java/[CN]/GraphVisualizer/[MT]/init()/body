  graphPanel.add(graphScrollPanel,BorderLayout.CENTER);
  graphPanel.add(graphInfoPanel,BorderLayout.SOUTH);
  this.add(mainPanel,BorderLayout.NORTH);
  this.add(graphPanel,BorderLayout.CENTER);
