{
  try {
    ScriptEngineManager sem=getManager();
    if (sem == null) {
      return;
    }
    sem.put("sampleEvent",event);
    sem.put("sampleResult",event.getResult());
    processFileOrScript(sem);
  }
 catch (  ScriptException e) {
    log.warn("Problem in JSR223 script " + e);
  }
catch (  IOException e) {
    log.warn("Problem in JSR223 script " + e);
  }
}
