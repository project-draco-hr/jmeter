{
  super();
  this.appProperties=JMeterUtils.getJMeterProperties();
  try {
    successLimit=Long.parseLong(appProperties.getProperty("mailer.successlimit"));
  }
 catch (  Exception ex) {
  }
  try {
    failureLimit=Long.parseLong(appProperties.getProperty("mailer.failurelimit"));
  }
 catch (  Exception ex) {
  }
  initGui();
}
