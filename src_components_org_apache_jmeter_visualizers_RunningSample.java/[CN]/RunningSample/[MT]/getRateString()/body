{
  long howLongRunning=lastTime - firstTime;
  if (howLongRunning == 0)   return ("N/A");
  double rate=(double)howLongRunning / counter / 1000.0;
  String unit="sec";
  if (rate < 1.0) {
    rate*=60.0;
    unit="min";
  }
  if (rate < 1.0) {
    rate*=60.0;
    unit="hour";
  }
  String rval=rateFormatter.format(rate) + "/" + unit;
  return (rval);
}
