{
  String aLabel=res.getSampleLabel();
  RunningSample s;
synchronized (labelMap) {
    s=(RunningSample)labelMap.get(aLabel);
    if (s == null) {
      s=new RunningSample(aLabel,runningSamples.size());
      runningSamples.add(s);
      labelMap.put(aLabel,s);
    }
  }
  s.addSample(res);
  total.addSample(res);
  this.fireDataChanged(s);
}
