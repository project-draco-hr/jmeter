{
synchronized (calc) {
    calc.addValue(res.getTime());
    Sample newS=new Sample(res.getSampleLabel(),res.getTime(),0,0,0,0,0,0,res.isSuccessful(),calc.getCount(),res.getTimeStamp());
    newS.setBytes(res.getResponseData().length);
    model.addRow(newS);
  }
  currentData=res.getTime();
  updateTextFields();
}
