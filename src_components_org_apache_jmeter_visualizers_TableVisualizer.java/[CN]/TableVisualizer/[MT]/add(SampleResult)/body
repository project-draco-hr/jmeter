{
  currentData=res.getTime();
synchronized (calc) {
    calc.addValue(currentData);
    int count=calc.getCount();
    Sample newS=new Sample(res.getSampleLabel(),res.getTime(),0,0,0,0,0,0,res.isSuccessful(),count,res.getEndTime(),res.getBytes(),res.getThreadName());
    model.addRow(newS);
  }
  updateTextFields();
}
