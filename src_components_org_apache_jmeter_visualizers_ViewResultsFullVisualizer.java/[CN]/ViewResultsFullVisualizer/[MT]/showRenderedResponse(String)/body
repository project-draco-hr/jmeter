{
  if (response == null) {
    results.setText("");
    return;
  }
  int htmlIndex=response.indexOf("<HTML>");
  if (htmlIndex < 0) {
    htmlIndex=response.indexOf("<html>");
  }
  if (htmlIndex < 0) {
    htmlIndex=0;
  }
  String html=response.substring(htmlIndex);
  results.setContentType("text/html");
  results.setText(html);
  results.setCaretPosition(0);
}
