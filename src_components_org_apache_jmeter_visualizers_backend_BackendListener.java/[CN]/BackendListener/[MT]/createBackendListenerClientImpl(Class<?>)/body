{
  if (clientClass == null) {
    return new ErrorBackendListenerClient();
  }
  BackendListenerClient client;
  try {
    client=(BackendListenerClient)clientClass.newInstance();
  }
 catch (  Exception e) {
    LOGGER.error("Exception creating: " + clientClass,e);
    client=new ErrorBackendListenerClient();
  }
  return client;
}
