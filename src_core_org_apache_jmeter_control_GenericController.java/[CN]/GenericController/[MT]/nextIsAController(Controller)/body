{
  Sampler sampler=null;
  try {
    sampler=controller.next();
  }
 catch (  StackOverflowError soe) {
    log.warn("StackOverflowError detected");
    throw new NextIsNullException("StackOverflowError detected",soe);
  }
  if (sampler == null) {
    currentReturnedNull(controller);
    sampler=next();
  }
  return sampler;
}
