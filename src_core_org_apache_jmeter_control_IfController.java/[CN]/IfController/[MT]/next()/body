{
  boolean result=true;
  if (isEvaluateAll() || isFirst()) {
    result=isUseExpression() ? evaluateExpression(getCondition()) : evaluateCondition(getCondition());
  }
  if (result) {
    return super.next();
  }
  try {
    reInitializeSubController();
    return nextIsNull();
  }
 catch (  NextIsNullException e1) {
    return null;
  }
}
