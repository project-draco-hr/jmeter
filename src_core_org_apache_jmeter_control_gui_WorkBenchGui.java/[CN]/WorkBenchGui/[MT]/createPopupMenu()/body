{
  JPopupMenu menu=new JPopupMenu();
  JMenu addMenu=MenuFactory.makeMenus(new String[]{MenuFactory.NON_TEST_ELEMENTS,MenuFactory.CONTROLLERS,MenuFactory.CONFIG_ELEMENTS,MenuFactory.TIMERS,MenuFactory.PRE_PROCESSORS,MenuFactory.SAMPLERS,MenuFactory.POST_PROCESSORS,MenuFactory.ASSERTIONS,MenuFactory.LISTENERS},JMeterUtils.getResString("add"),ActionNames.ADD);
  menu.add(addMenu);
  MenuFactory.addPasteResetMenu(menu);
  MenuFactory.addFileMenu(menu);
  return menu;
}
