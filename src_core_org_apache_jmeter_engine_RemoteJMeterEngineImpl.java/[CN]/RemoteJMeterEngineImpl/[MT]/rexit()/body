{
  log.info("Exitting");
  backingEngine.exit();
  Registry reg=LocateRegistry.getRegistry(this.rmiPort);
  try {
    reg.unbind(JMETER_ENGINE_RMI_NAME);
  }
 catch (  NotBoundException e) {
    log.warn(JMETER_ENGINE_RMI_NAME + " is not bound",e);
  }
  log.info("Unbound from registry");
  JMeterUtils.helpGC();
}
