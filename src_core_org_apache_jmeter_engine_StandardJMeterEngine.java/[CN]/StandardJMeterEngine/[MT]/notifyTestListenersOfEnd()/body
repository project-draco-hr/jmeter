{
  Iterator iter=testListeners.getSearchResults().iterator();
  while (iter.hasNext()) {
    TestListener tl=(TestListener)iter.next();
    if (tl instanceof TestBean)     ((TestBean)tl).prepare();
    if (host == null) {
      tl.testEnded();
    }
 else {
      tl.testEnded(host);
    }
  }
  log.info("Test has ended");
}
