{
synchronized (allThreads) {
    Iterator<JMeterThread> iter=new HashSet<JMeterThread>(allThreads.keySet()).iterator();
    while (iter.hasNext()) {
      JMeterThread item=iter.next();
      item.stop();
      item.interrupt();
      Thread t=allThreads.get(item);
      t.interrupt();
    }
  }
}
