{
  log.info("Ending thread " + thread.getThreadName());
  allThreads.remove(thread);
  if (!startingGroups && allThreads.size() == 0) {
    new Thread(){
      @Override public void run(){
        log.info("Stopping test");
        notifyTestListenersOfEnd(testListenersSave);
      }
    }
.start();
  }
}
