{
  Iterator iter=new LinkedList(tree.list()).iterator();
  while (iter.hasNext()) {
    JMeterTreeNode item=(JMeterTreeNode)iter.next();
    if (item.isEnabled()) {
      convertSubTree(tree.getTree(item));
      TestElement testElement=item.createTestElement();
      tree.replace(item,testElement);
    }
 else {
      tree.remove(item);
    }
  }
}
