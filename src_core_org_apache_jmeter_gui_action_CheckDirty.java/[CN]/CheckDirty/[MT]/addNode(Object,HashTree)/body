{
  log.debug("Node is class:" + node.getClass());
  JMeterGUIComponent treeNode=(JMeterGUIComponent)node;
  if (checkMode) {
    if (previousGuiItems.containsKey(treeNode)) {
      if (!previousGuiItems.get(treeNode).equals(treeNode.createTestElement())) {
        dirty=true;
      }
    }
 else {
      dirty=true;
    }
  }
 else   if (removeMode) {
    previousGuiItems.remove(treeNode);
  }
 else {
    previousGuiItems.put(treeNode,treeNode.createTestElement().clone());
  }
}
