{
  JMeterGUIComponent gui=null;
  fixTestElement(item);
  try {
    gui=(JMeterGUIComponent)Class.forName((String)item.getProperty(TestElement.GUI_CLASS)).newInstance();
  }
 catch (  Exception e) {
    log.warn("Couldn't get gui for " + item,e);
    gui=new WorkBenchGui();
  }
  gui.configure(item);
  return gui;
}
