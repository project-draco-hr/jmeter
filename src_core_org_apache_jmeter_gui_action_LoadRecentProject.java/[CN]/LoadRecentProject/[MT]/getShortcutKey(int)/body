{
  int shortKey=-1;
switch (index + 1) {
case 1:
    shortKey=KeyEvent.VK_1;
  break;
case 2:
shortKey=KeyEvent.VK_2;
break;
case 3:
shortKey=KeyEvent.VK_3;
break;
case 4:
shortKey=KeyEvent.VK_4;
break;
case 5:
shortKey=KeyEvent.VK_5;
break;
case 6:
shortKey=KeyEvent.VK_6;
break;
case 7:
shortKey=KeyEvent.VK_7;
break;
case 8:
shortKey=KeyEvent.VK_8;
break;
case 9:
shortKey=KeyEvent.VK_9;
break;
default :
break;
}
return shortKey;
}
