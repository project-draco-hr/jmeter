{
  boolean result=false;
  for (  String searchableToken : textTokens) {
    if (!StringUtils.isEmpty(searchableToken)) {
      if (caseSensitive) {
        result=searchableToken.indexOf(textToSearch) >= 0;
      }
 else {
        result=searchableToken.toLowerCase().indexOf(textToSearch) >= 0;
      }
      if (result) {
        return result;
      }
    }
  }
  return false;
}
