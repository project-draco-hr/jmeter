{
  for (int i=0; i < nodes.length; i++) {
    JMeterTreeNode node=nodes[i];
    Object userObject=node.getUserObject();
    if (!except.isInstance(userObject)) {
      for (int j=0; j < classes.length; j++) {
        if (classes[j].isInstance(userObject)) {
          return true;
        }
      }
    }
  }
  return false;
}
