{
  boolean tmpDirCreated=false;
  if (!tmpDir.exists()) {
    tmpDirCreated=tmpDir.mkdir();
    if (!tmpDirCreated) {
      String message=String.format("Cannot create temporary directory \"%s\".",tmpDir);
      log.error(message);
      throw new GenerationException(message);
    }
  }
  return tmpDirCreated;
}
