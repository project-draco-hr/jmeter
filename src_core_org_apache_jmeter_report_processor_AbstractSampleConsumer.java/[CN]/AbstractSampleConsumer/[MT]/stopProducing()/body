{
  for (  SampleConsumer consumer : this.sampleConsumers) {
    try {
      consumer.stopConsuming();
    }
 catch (    Exception e) {
      LOG.error("stopProducing(): Consumer failed with message :" + e.getMessage(),e);
      throw new SampleException(e);
    }
  }
  if (LOG.isInfoEnabled()) {
    LOG.info("stopProducing(): " + getName() + " produced "+ producedSampleCount+ " samples");
  }
}
