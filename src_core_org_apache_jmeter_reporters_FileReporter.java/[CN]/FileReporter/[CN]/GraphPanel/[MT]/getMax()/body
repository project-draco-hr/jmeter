{
  float maxValue=0;
  for (int t=0; t < keys.size(); t++) {
    String key=keys.get(t);
    List<Integer> temp=data.get(key);
    for (int j=0; j < temp.size(); j++) {
      float f=temp.get(j).intValue();
      maxValue=Math.max(f,maxValue);
    }
  }
  return (float)(maxValue + maxValue * 0.1);
}
