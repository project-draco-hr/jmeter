{
  log.info("Test Ended on " + host);
  try {
    Iterator i=sampleStore.iterator();
    while (i.hasNext()) {
      SampleEvent se=(SampleEvent)i.next();
      listener.sampleOccurred(se);
    }
    listener.testEnded(host);
    sampleStore=null;
  }
 catch (  Throwable ex) {
    log.error("testEnded(host)",ex);
  }
}
