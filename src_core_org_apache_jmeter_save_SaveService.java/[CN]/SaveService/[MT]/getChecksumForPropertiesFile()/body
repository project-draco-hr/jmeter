{
  MessageDigest md=MessageDigest.getInstance("SHA1");
  try (BufferedReader reader=new BufferedReader(new InputStreamReader(new FileInputStream(JMeterUtils.getJMeterHome() + JMeterUtils.getPropDefault(SAVESERVICE_PROPERTIES,SAVESERVICE_PROPERTIES_FILE))))){
    String line=null;
    while ((line=reader.readLine()) != null) {
      md.update(line.getBytes());
    }
  }
   return JOrphanUtils.baToHexString(md.digest());
}
