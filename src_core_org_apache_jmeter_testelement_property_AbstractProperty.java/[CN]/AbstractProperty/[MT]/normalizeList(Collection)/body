{
  Iterator iter=coll.iterator();
  Collection newColl=null;
  while (iter.hasNext()) {
    Object item=iter.next();
    if (newColl == null) {
      try {
        newColl=coll.getClass().newInstance();
      }
 catch (      Exception e) {
        log.error("Bad collection",e);
        return coll;
      }
    }
    newColl.add(convertObject(item));
  }
  if (newColl != null) {
    return newColl;
  }
 else {
    return coll;
  }
}
