{
  try {
    Collection newCol=(Collection)value.getClass().newInstance();
    PropertyIterator iter=iterator();
    while (iter.hasNext()) {
      newCol.add(iter.next().clone());
    }
    return newCol;
  }
 catch (  Exception e) {
    log.error("Couldn't clone collection",e);
    return value;
  }
}
