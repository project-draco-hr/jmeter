{
  int sum=0;
  Iterator iter=timers.iterator();
  while (iter.hasNext()) {
    Timer timer=(Timer)iter.next();
    if (timer instanceof TestBean)     ((TestBean)timer).prepare();
    sum+=timer.delay();
  }
  if (sum > 0) {
    try {
      Thread.sleep(sum);
    }
 catch (    InterruptedException e) {
      log.error("",e);
    }
  }
}
