{
  JPanel panel=new JPanel(new BorderLayout(5,0));
  JLabel label=new JLabel("endtime");
  label.setLabelFor(endtime);
  panel.add(label,BorderLayout.WEST);
  Date today=new Date();
  end=new JSpinner(new SpinnerDateModel(today,null,null,Calendar.MONTH));
  endtime=new DateEditor(end,"dd/MM/yy HH:mm:ss");
  end.setEditor(endtime);
  panel.add(end,BorderLayout.CENTER);
  return panel;
}
