{
  System.out.println("Reading responses from server ...");
  int x=0;
  try {
    while ((x=is.read()) > -1) {
      char c=(char)x;
      System.out.print(c);
    }
  }
 catch (  IOException e) {
  }
 finally {
    System.out.println("... disconnected from server.");
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
