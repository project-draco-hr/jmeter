{
  if (logger != null) {
    try {
      set("log",logger);
    }
 catch (    JMeterException e) {
      log.error("Can't set logger variable",e);
      throw e;
    }
  }
  if (initFile != null && initFile.length() > 0) {
    String fileToUse=initFile;
    File in=new File(fileToUse);
    if (!in.exists()) {
      fileToUse=JMeterUtils.getJMeterHome() + File.separator + "bin"+ File.separator+ initFile;
      in=new File(fileToUse);
      if (!in.exists()) {
        throw new FileNotFoundException(initFile);
      }
    }
    if (!in.canRead()) {
      throw new IOException("Cannot read" + fileToUse);
    }
    source(fileToUse);
  }
}
