{
  List tempList;
  if ((tempList=(List)data.get(column)) == null) {
    tempList=new ArrayList();
    data.put(column,tempList);
  }
  if (currentPos == -1)   currentPos=0;
  if (currentPos >= size) {
    size++;
    tempList.add(value);
  }
 else   if (currentPos >= tempList.size()) {
    tempList.add(value);
  }
 else   tempList.set(currentPos,value);
}
