{
  if (key == null) {
    return null;
  }
  key=key.replace(' ','_');
  key=key.toLowerCase();
  String resString=null;
  try {
    resString=resources.getString(key);
  }
 catch (  MissingResourceException mre) {
    log.warn("ERROR! Resource string not found: [" + key + "]");
    resString=defaultValue;
  }
  return resString;
}
