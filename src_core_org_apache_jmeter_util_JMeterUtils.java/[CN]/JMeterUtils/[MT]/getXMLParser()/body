{
  XMLReader reader=null;
  final String parserName=getPropDefault("xml.parser","org.apache.xerces.parsers.SAXParser");
  try {
    reader=(XMLReader)instantiate(parserName,"org.xml.sax.XMLReader");
  }
 catch (  Exception e) {
    reader=(XMLReader)instantiate(parserName,"org.xml.sax.XMLReader");
  }
  return reader;
}
