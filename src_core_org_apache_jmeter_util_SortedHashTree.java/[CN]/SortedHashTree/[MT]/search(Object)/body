{
  SortedHashTree temp=null;
  if (data.containsKey(key))   temp=(SortedHashTree)data.get(key);
 else {
    Iterator it=list().iterator();
    while (it.hasNext()) {
      if (temp == null) {
        temp=((SortedHashTree)it.next()).search(key);
        break;
      }
 else       break;
    }
  }
  return temp;
}
