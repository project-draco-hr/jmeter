{
  Tidy tidy=new Tidy();
  tidy.setInputEncoding("UTF8");
  tidy.setOutputEncoding("UTF8");
  tidy.setQuiet(quiet);
  tidy.setShowWarnings(showWarnings);
  tidy.setMakeClean(true);
  tidy.setXmlTags(isXml);
  if (stringWriter != null) {
    tidy.setErrout(new PrintWriter(stringWriter));
  }
  return tidy;
}
