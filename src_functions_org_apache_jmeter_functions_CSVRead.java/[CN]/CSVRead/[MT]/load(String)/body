{
  FileDataContainer myfileData=new FileDataContainer();
  openFile(fileName);
  if (null != myBread) {
    try {
      String line=myBread.readLine();
      while (line != null) {
        myfileData.addLine(line);
        line=myBread.readLine();
      }
      myBread.close();
      setFileData(myfileData);
      return myfileData;
    }
 catch (    java.io.IOException e) {
      log.error("load(" + fileName + ")",e);
      throw e;
    }
  }
  return myfileData;
}
