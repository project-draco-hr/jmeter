{
  String machineName=JMeterUtils.getLocalHostName();
  if (values.length >= 1) {
    JMeterVariables vars=getVariables();
    if (vars != null) {
      String varName=((CompoundVariable)values[0]).execute().trim();
      if (varName.length() > 0) {
        vars.put(varName,machineName);
      }
    }
  }
  return machineName;
}
