{
  Object source;
  String name;
  source=event.getSource();
  if (source == mLinkBean) {
    if (!mLinkBean.getURL().equals(mStringBean.getURL()))     mStringBean.setConnection(mLinkBean.getConnection());
  }
 else   if (source == mStringBean) {
    if (!mStringBean.getURL().equals(mLinkBean.getURL()))     mLinkBean.setConnection(mStringBean.getConnection());
    name=event.getPropertyName();
    if (name.equals(StringBean.PROP_LINKS_PROPERTY))     mLinks.setSelected(((Boolean)event.getNewValue()).booleanValue());
 else     if (name.equals(StringBean.PROP_COLLAPSE_PROPERTY))     mCollapse.setSelected(((Boolean)event.getNewValue()).booleanValue());
 else     if (name.equals(StringBean.PROP_REPLACE_SPACE_PROPERTY))     mNobreak.setSelected(((Boolean)event.getNewValue()).booleanValue());
  }
}
