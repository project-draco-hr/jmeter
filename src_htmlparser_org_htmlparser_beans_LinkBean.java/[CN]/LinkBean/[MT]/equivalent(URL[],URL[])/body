{
  boolean ret;
  ret=false;
  if ((null == array1) && (null == array2))   ret=true;
 else   if ((null != array1) && (null != array2))   if (array1.length == array2.length) {
    ret=true;
    for (int i=0; i < array1.length && ret; i++)     if (!(array1[i] == array2[i]))     ret=false;
  }
  return (ret);
}
