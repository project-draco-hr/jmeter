{
  if (0 >= args.length)   System.out.println("Usage: java -classpath htmlparser.jar org.htmlparser.beans.StringBean <http://whatever_url>");
 else {
    StringBean sb=new StringBean();
    sb.setLinks(false);
    sb.setReplaceNonBreakingSpaces(true);
    sb.setCollapse(true);
    sb.setURL(args[0]);
    System.out.println(sb.getStrings());
  }
}
