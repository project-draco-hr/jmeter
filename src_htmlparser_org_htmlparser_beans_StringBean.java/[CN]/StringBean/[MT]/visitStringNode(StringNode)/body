{
  if (!mIsScript) {
    String text=string.getText();
    if (!mIsPre) {
      text=Translate.decode(text);
      if (getReplaceNonBreakingSpaces())       text=text.replace('\u00a0',' ');
      if (getCollapse())       collapse(mBuffer,text);
 else       mBuffer.append(text);
    }
 else     mBuffer.append(text);
  }
}
