{
  int length;
  boolean parse;
  char ch;
  StringBuffer ret;
  length=s.length();
  ret=new StringBuffer(length);
  parse=true;
  for (int i=0; i < length && parse; i++) {
    ch=s.charAt(i);
    if (Character.isWhitespace(ch) || ch == '=')     parse=false;
 else     ret.append(Character.toUpperCase(ch));
  }
  return (ret.toString());
}
