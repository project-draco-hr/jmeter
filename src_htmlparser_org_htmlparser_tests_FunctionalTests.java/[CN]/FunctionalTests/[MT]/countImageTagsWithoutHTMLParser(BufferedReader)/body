{
  String line;
  int imgTagCount=0;
  do {
    line=reader.readLine();
    if (line != null) {
      String newline=line.toUpperCase();
      int fromIndex=-1;
      do {
        fromIndex=newline.indexOf("<IMG",fromIndex + 1);
        if (fromIndex != -1) {
          imgTagCount++;
        }
      }
 while (fromIndex != -1);
    }
  }
 while (line != null);
  return imgTagCount;
}
