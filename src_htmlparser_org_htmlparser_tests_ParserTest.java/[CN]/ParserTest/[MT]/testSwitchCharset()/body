{
  Parser parser;
  String url="http://htmlparser.sourceforge.net/test/gb2312Charset.html";
  int i;
  Node[] nodes;
  parser=new Parser(url);
  i=0;
  nodes=new Node[30];
  for (NodeIterator e=parser.elements(); e.hasMoreNodes(); )   nodes[i++]=e.nextNode();
  assertEquals("Expected nodes",14,i);
}
