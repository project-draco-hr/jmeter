{
  try {
    result=false;
    Node linkTag[]=parser.extractAllNodesThatAre(LinkTag.class);
    link1=(LinkTag)linkTag[0];
    link2=(LinkTag)linkTag[1];
    if (id < max / 2) {
      if (link1.getLink().equals("/cgi-bin/view_search?query_text=postdate>20020701&txt_clr=White&bg_clr=Red&url=http://localhost/Testing/Report1.html") && link2.getLink().equals("http://normallink.com/sometext.html"))       result=true;
    }
 else {
      if (link1.getLink().equals("http://normallink.com/sometext.html") && link2.getLink().equals("http://normallink.com/sometext.html"))       result=true;
    }
  }
 catch (  ParserException e) {
    System.err.println("Parser Exception");
    e.printStackTrace();
  }
 finally {
    testProgress+=id;
  }
}
