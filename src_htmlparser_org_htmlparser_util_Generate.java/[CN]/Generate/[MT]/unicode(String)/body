{
  int code;
  if (string.startsWith("\"&#") && string.endsWith(";\"")) {
    string=string.substring(3,string.length() - 2);
    try {
      code=Integer.parseInt(string);
      string="new Character ('\\u" + pad(Integer.toHexString(code),'0',4) + "')";
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    return (string);
  }
 else   return (string);
}
