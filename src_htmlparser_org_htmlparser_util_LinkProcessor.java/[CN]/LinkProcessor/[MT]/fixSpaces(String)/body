{
  int index;
  int length;
  char ch;
  StringBuffer returnURL;
  index=url.indexOf(' ');
  if (-1 != index) {
    length=url.length();
    returnURL=new StringBuffer(length * 3);
    returnURL.append(url.substring(0,index));
    for (int i=index; i < length; i++) {
      ch=url.charAt(i);
      if (ch == ' ')       returnURL.append("%20");
 else       returnURL.append(ch);
    }
    url=returnURL.toString();
  }
  return (url);
}
