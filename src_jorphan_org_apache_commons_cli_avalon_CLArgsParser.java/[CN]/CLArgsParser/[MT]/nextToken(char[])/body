{
  m_ch=getChar();
  if (isSeparator(m_ch,separators)) {
    m_ch=getChar();
    return new Token(TOKEN_SEPARATOR,null);
  }
  final StringBuffer sb=new StringBuffer();
  do {
    sb.append(m_ch);
    m_ch=getChar();
  }
 while (!isSeparator(m_ch,separators));
  return new Token(TOKEN_STRING,sb.toString());
}
