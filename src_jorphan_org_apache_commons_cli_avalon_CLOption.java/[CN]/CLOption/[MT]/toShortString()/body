{
  final StringBuilder sb=new StringBuilder();
  final char id=(char)m_descriptor.getId();
  if (id != TEXT_ARGUMENT) {
    sb.append("-");
    sb.append(id);
  }
  if (null != m_arguments) {
    if (id != TEXT_ARGUMENT) {
      sb.append("=");
    }
    sb.append(Arrays.asList(m_arguments));
  }
  return sb.toString();
}
