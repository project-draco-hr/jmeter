{
  final StringBuffer sb=new StringBuffer();
  sb.append("-");
  final char id=(char)m_descriptor.getId();
  if (id == 0) {
    sb.append("-");
  }
 else {
    sb.append(id);
  }
  if (null != m_arguments) {
    sb.append("=");
    sb.append(Arrays.asList(m_arguments));
  }
  return sb.toString();
}
