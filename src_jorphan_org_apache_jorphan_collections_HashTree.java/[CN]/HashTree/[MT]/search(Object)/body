{
  HashTree temp=null;
  if (data.containsKey(key)) {
    temp=(HashTree)data.get(key);
  }
 else {
    Iterator it=list().iterator();
    while (it.hasNext()) {
      if (temp == null) {
        temp=((HashTree)it.next()).search(key);
        break;
      }
 else {
        break;
      }
    }
  }
  return temp;
}
