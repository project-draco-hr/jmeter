{
  Writer wt;
  isWriterSystemOut=false;
  try {
    wt=new FileWriter(logFile);
  }
 catch (  Exception e) {
    System.out.println(propName + "=" + logFile+ " "+ e.toString());
    System.out.println("[" + propName + "-> System.out]");
    isWriterSystemOut=true;
    wt=new PrintWriter(System.out);
  }
  return wt;
}
