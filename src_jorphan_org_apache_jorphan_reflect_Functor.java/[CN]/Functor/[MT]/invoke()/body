{
  try {
    return createMethod(getTypes()).invoke(invokee,getArgs());
  }
 catch (  Exception e) {
    final String message="Trouble functing method: " + methodName + " invokee: "+ invokee.getClass().getName();
    log.warn(message,e);
    throw new org.apache.jorphan.util.JMeterError(message,e);
  }
}
