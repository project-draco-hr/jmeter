{
  if (clazz != null && method != null) {
    log.info("class " + clazz.getClass().getName() + " method name is "+ method);
    try {
      return clazz.getClass().getMethod(method,new Class[0]);
    }
 catch (    NoSuchMethodException e) {
      log.warn(e.getMessage());
    }
  }
  return null;
}
