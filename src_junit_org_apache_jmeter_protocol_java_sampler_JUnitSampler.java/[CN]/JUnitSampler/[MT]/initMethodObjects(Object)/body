{
  SETUP_METHOD=null;
  TDOWN_METHOD=null;
  if (!getDoNotSetUpTearDown()) {
    SETUP_METHOD=getJunit4() ? getMethodWithAnnotation(testObject,Before.class) : getMethod(testObject,SETUP);
    TDOWN_METHOD=getJunit4() ? getMethodWithAnnotation(testObject,After.class) : getMethod(testObject,TEARDOWN);
  }
}
