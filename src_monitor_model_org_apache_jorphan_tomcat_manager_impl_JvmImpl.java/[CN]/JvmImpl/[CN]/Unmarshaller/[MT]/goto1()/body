{
  int idx;
  state=1;
  idx=context.getAttribute("","free");
  if (idx >= 0) {
    context.consumeAttribute(idx);
    return;
  }
  idx=context.getAttribute("","total");
  if (idx >= 0) {
    context.consumeAttribute(idx);
    return;
  }
  idx=context.getAttribute("","max");
  if (idx >= 0) {
    context.consumeAttribute(idx);
    return;
  }
}
