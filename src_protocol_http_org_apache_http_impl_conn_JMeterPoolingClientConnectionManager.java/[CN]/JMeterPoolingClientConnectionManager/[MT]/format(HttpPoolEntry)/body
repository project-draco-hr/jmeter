{
  final StringBuilder buf=new StringBuilder();
  buf.append("[id: ").append(entry.getId()).append("]");
  buf.append("[route: ").append(entry.getRoute()).append("]");
  final Object state=entry.getState();
  if (state != null) {
    buf.append("[state: ").append(state).append("]");
  }
  return buf.toString();
}
