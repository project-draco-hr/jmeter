{
  domain.setText("");
  if (notConfigOnly) {
    followRedirects.setSelected(false);
    autoRedirects.setSelected(true);
    method.setText(HTTPSamplerBase.DEFAULT_METHOD);
    useKeepAlive.setSelected(true);
    useMultipartForPost.setSelected(false);
  }
  path.setText("");
  port.setText("");
  protocol.setText("");
  contentEncoding.setText("");
  argsPanel.clear();
}
