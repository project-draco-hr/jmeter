  if (event.getSource() == generatorClassName) {
    SAMPLER.setGeneratorClassName(generatorClassName.getText());
    handleGeneratorEvent();
  }
  if (event.getSource() == HOSTNAME) {
  }
  if (event.getSource() == PORT) {
