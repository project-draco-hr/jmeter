{
  super.clearGui();
  getImages.setSelected(false);
  concurrentDwn.setSelected(false);
  concurrentPool.setText(String.valueOf(HTTPSamplerBase.CONCURRENT_POOL_SIZE));
  enableConcurrentDwn(false);
  isMon.setSelected(false);
  useMD5.setSelected(false);
  urlConfigGui.clear();
  embeddedRE.setText("");
  if (!isAJP) {
    sourceIpAddr.setText("");
    sourceIpType.setSelectedItem(HTTPSamplerBase.getSourceTypeList().get(HTTPSamplerBase.SOURCE_TYPE_IP_HOSTNAME));
  }
}
