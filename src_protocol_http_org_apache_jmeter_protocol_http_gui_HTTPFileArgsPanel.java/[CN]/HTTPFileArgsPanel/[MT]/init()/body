{
  JPanel p=this;
  p.setLayout(new BorderLayout());
  if (tableLabel != null) {
    p.add(makeLabelPanel(),BorderLayout.NORTH);
  }
  p.add(makeMainPanel(),BorderLayout.CENTER);
  p.add(Box.createVerticalStrut(70),BorderLayout.WEST);
  p.add(makeButtonPanel(),BorderLayout.SOUTH);
  table.revalidate();
}
