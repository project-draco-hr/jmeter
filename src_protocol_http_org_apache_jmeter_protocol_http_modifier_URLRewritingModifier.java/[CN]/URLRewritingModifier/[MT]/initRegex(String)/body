{
  case1=JMeterUtils.getPatternCache().getPattern(argName + "=([^\"'>& \n\r;]*)[& \\n\\r\"'>;]?$?",Perl5Compiler.MULTILINE_MASK);
  case2=JMeterUtils.getPatternCache().getPattern("[Nn][Aa][Mm][Ee]=\"" + argName + "\"[^>]+[vV][Aa][Ll][Uu][Ee]=\"([^\"]*)\"",Perl5Compiler.MULTILINE_MASK);
  case3=JMeterUtils.getPatternCache().getPattern("[vV][Aa][Ll][Uu][Ee]=\"([^\"]*)\"[^>]+[Nn][Aa][Mm][Ee]=\"" + argName + "\"",Perl5Compiler.MULTILINE_MASK);
  case4=JMeterUtils.getPatternCache().getPattern(argName + "([^\"'>& \n\r]*)[& \\n\\r\"'>]?$?",Perl5Compiler.MULTILINE_MASK);
}
