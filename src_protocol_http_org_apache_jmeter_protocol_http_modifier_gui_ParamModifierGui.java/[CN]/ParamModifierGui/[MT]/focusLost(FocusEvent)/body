{
  String name=((Component)evt.getSource()).getName();
  if (evt.isTemporary()) {
    return;
  }
 else   if (name.equals(LOWERBOUND)) {
    checkTextField(evt,ZERO);
  }
 else   if (name.equals(UPPERBOUND)) {
    checkTextField(evt,ZERO);
  }
 else   if (name.equals(INCREMENT)) {
    checkTextField(evt,ZERO);
  }
}
