{
  String htmlParserClassName=JMeterUtils.getPropDefault("htmlParser.className","org.apache.jmeter.protocol.http.parser.HtmlParserHTMLParser");
  try {
    parser=(HTMLParser)Class.forName(htmlParserClassName).newInstance();
  }
 catch (  InstantiationException e) {
    throw new Error(e);
  }
catch (  IllegalAccessException e) {
    throw new Error(e);
  }
catch (  ClassNotFoundException e) {
    throw new Error(e);
  }
  log.info("Using " + htmlParserClassName);
}
