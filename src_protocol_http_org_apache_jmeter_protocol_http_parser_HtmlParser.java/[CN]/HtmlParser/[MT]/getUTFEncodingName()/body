{
  log.debug("Start : getUTFEncodingName1");
  if (utfEncodingName == null) {
    String versionNum=System.getProperty("java.version");
    if (log.isDebugEnabled())     log.debug("getUTFEncodingName1 : versionNum - " + versionNum);
    if (versionNum.startsWith("1.1"))     utfEncodingName="UTF8";
 else     utfEncodingName="UTF-8";
  }
  if (log.isDebugEnabled())   log.debug("getUTFEncodingName1 : Returning utfEncodingName - " + utfEncodingName);
  log.debug("End : getUTFEncodingName1");
  return utfEncodingName;
}
