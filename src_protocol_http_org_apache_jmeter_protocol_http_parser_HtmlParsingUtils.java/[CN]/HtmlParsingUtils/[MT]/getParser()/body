{
  log.debug("Start : getParser1");
  Tidy tidy=new Tidy();
  tidy.setInputEncoding("UTF8");
  tidy.setOutputEncoding("UTF8");
  tidy.setQuiet(true);
  tidy.setShowWarnings(false);
  if (log.isDebugEnabled()) {
    log.debug("getParser1 : tidy parser created - " + tidy);
  }
  log.debug("End : getParser1");
  return tidy;
}
