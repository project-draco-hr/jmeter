{
  String str=url;
  int i=str.indexOf("//");
  if (i > 0) {
    str=str.substring(i + 2);
  }
  i=str.indexOf("/");
  if (0 < i) {
    str=str.substring(0,i);
  }
  i=str.lastIndexOf(":");
  if (0 < i) {
    return Integer.parseInt(str.substring(i + 1).trim());
  }
  return HTTPSamplerBase.UNSPECIFIED_PORT;
}
