{
  JMeterTreeModel treeModel=GuiPackage.getInstance().getTreeModel();
  List<JMeterTreeNode> nodes=treeModel.getNodesOfType(type);
  Iterator<JMeterTreeNode> iter=nodes.iterator();
  while (iter.hasNext()) {
    JMeterTreeNode node=iter.next();
    if (node.isEnabled()) {
      return node;
    }
  }
  return null;
}
