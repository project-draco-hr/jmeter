{
  ValueReplacer replacer=GuiPackage.getInstance().getReplacer();
  modifyTestElement(model);
  try {
    replacer.replaceValues(model);
    model.startProxy();
    start.setEnabled(false);
    stop.setEnabled(true);
    restart.setEnabled(false);
  }
 catch (  InvalidVariableException e) {
    JOptionPane.showMessageDialog(this,JMeterUtils.getResString("invalid_variables"),"Error",JOptionPane.ERROR_MESSAGE);
  }
catch (  BindException e) {
    JOptionPane.showMessageDialog(this,JMeterUtils.getResString("proxy_daemon_bind_error"),"Error",JOptionPane.ERROR_MESSAGE);
  }
catch (  IOException e) {
    JOptionPane.showMessageDialog(this,JMeterUtils.getResString("proxy_daemon_error"),"Error",JOptionPane.ERROR_MESSAGE);
  }
}
