{
  if (this.getParserClassName() != null && this.getParserClassName().length() > 0) {
    try {
      if (PARSERCLASS == null) {
        PARSERCLASS=Class.forName(this.getParserClassName());
      }
      return true;
    }
 catch (    ClassNotFoundException e) {
      return false;
    }
  }
 else {
    return false;
  }
}
