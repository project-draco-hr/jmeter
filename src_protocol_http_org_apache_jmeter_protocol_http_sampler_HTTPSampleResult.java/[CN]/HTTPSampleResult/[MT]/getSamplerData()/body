{
  StringBuffer sb=new StringBuffer();
  sb.append(getHTTPMethod());
  URL u=getURL();
  if (u != null) {
    sb.append(' ');
    sb.append(u.toString());
    if ("POST".equals(getHTTPMethod())) {
      sb.append(getQueryString());
    }
    sb.append("\n");
    sb.append(getRequestHeaders());
    sb.append(getCookies());
  }
  return sb.toString();
}
