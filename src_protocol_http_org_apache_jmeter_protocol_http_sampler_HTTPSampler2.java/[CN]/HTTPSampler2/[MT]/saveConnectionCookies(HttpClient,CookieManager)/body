{
  if (cookieManager != null) {
    org.apache.commons.httpclient.Cookie[] c=client.getState().getCookies();
    for (int i=0; i < c.length; i++) {
      Date exp=c[i].getExpiryDate();
      Cookie cookie=new Cookie(c[i].getName(),c[i].getValue(),c[i].getDomain(),c[i].getPath(),c[i].getSecure(),exp == null ? 0 : exp.getTime() / 1000);
      cookieManager.add(cookie);
    }
  }
}
