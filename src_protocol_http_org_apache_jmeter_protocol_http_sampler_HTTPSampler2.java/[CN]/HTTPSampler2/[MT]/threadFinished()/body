{
  log.debug("Thread Finished");
  Map map=(Map)httpClients.get();
  HttpConnection conn;
  if (map != null) {
    for (Iterator it=map.entrySet().iterator(); it.hasNext(); ) {
      Object obj=it.next();
      conn=(HttpConnection)((Map.Entry)obj).getValue();
      conn.close();
    }
    map.clear();
  }
  httpClients.set(null);
}
