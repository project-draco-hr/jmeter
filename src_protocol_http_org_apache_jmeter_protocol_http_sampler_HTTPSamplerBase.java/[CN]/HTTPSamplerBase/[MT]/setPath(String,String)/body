{
  if (GET.equals(getMethod()) || DELETE.equals(getMethod())) {
    int index=path.indexOf(QRY_PFX);
    if (index > -1) {
      setProperty(PATH,path.substring(0,index));
      parseArguments(path.substring(index + 1),contentEncoding);
    }
 else {
      setProperty(PATH,path);
    }
  }
 else {
    setProperty(PATH,path);
  }
}
