{
  StringBuffer sb=new StringBuffer(150);
  try {
    sb.append(this.getUrl().toString());
  }
 catch (  MalformedURLException e) {
    sb.append(e.getLocalizedMessage());
  }
  sb.append("\nXML Data: ");
  String xml=getXmlData();
  if (xml.length() > 100) {
    sb.append(xml.substring(0,100));
  }
 else {
    sb.append(xml);
  }
  return sb.toString();
}
