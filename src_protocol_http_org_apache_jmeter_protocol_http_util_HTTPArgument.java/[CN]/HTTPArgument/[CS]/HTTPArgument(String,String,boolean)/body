{
  setAlwaysEncoded(true);
  if (alreadyEncoded) {
    try {
      name=URLDecoder.decode(name,"UTF-8");
      value=URLDecoder.decode(value.toString(),"UTF-8");
    }
 catch (    UnsupportedEncodingException e) {
      log.error("UTF-8 encoding not supported!");
      throw new Error(e);
    }
  }
  setName(name);
  setValue(value);
  setMetaData("=");
}
