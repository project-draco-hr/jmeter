{
  if (value == null || !value.equals(getProperty(key))) {
    if (Argument.NAME.equals(key)) {
      if (value == null) {
        encodeName("");
      }
 else {
        encodeName(value.toString());
      }
    }
 else     if (Argument.VALUE.equals(key)) {
      encodeValue(value);
    }
    super.setProperty(key,value);
  }
}
