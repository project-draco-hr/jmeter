{
  StringBuilder str=new StringBuilder();
  PropertyIterator iter=getHTTPFileArgsCollection().iterator();
  while (iter.hasNext()) {
    HTTPFileArg file=(HTTPFileArg)iter.next().getObjectValue();
    str.append(file.getPath());
    if (iter.hasNext()) {
      str.append("&");
    }
  }
  return str.toString();
}
