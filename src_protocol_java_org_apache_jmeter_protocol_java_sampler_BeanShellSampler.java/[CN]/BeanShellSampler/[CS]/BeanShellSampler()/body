{
  String init="";
  try {
    bshInterpreter=new Interpreter();
    init=JMeterUtils.getPropDefault(INIT_FILE,null);
    if (init != null) {
      try {
        bshInterpreter.source(null);
        bshInterpreter.set("log",log);
      }
 catch (      IOException e) {
        log.warn("Error processing init file " + init + " "+ e);
      }
catch (      Exception e) {
        log.warn("Error processing init file " + init + " "+ e);
      }
    }
  }
 catch (  NoClassDefFoundError e) {
    bshInterpreter=null;
  }
}
