{
  log.debug(whoAmI() + "\ttestEnded");
synchronized (javaClientAndContextSet) {
    for (    Object[] javaClientAndContext : javaClientAndContextSet) {
      JavaSamplerClient jsClient=(JavaSamplerClient)javaClientAndContext[0];
