{
  DBKey key=new DBKey(driver,url,username,password);
synchronized (poolMap) {
    if (!poolMap.containsKey(key)) {
      if (registerDriver(driver)) {
        poolMap.put(key,createConnectionPool(key,maxUsage,maxConnections));
      }
 else {
        return null;
      }
    }
  }
  return key;
}
