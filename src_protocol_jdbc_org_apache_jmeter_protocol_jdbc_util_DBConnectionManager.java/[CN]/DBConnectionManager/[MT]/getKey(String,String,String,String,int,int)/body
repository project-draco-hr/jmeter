{
  DBKey key=new DBKey();
  if (registerDriver(driver)) {
    key.setDriver(driver);
    key.setMaxConnections(maxConnections);
    key.setMaxUsage(maxUsage);
    key.setPassword(password);
    key.setUrl(url);
    key.setUsername(username);
    if (!connections.containsKey(key))     setup(key);
  }
 else   key=null;
  return key;
}
