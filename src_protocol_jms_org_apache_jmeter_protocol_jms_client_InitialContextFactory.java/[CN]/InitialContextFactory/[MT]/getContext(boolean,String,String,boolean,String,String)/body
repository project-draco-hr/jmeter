{
  if (useProps) {
    try {
      return new InitialContext();
    }
 catch (    NoClassDefFoundError e) {
      throw new NamingException(e.toString());
    }
catch (    Exception e) {
      throw new NamingException(e.toString());
    }
  }
 else {
    return lookupContextNoCache(initialContextFactory,providerUrl,useAuth,securityPrincipal,securityCredentials);
  }
}
