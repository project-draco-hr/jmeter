{
  super();
  Context ctx=initJNDI(useProps,initialContextFactory,providerUrl,useAuth,securityPrincipal,securityCredentials);
  if (ctx != null) {
    initConnection(ctx,connfactory,topic);
  }
 else {
    log.error("Could not initialize JNDI Initial Context Factory");
  }
  isValid=publisher != null;
}
