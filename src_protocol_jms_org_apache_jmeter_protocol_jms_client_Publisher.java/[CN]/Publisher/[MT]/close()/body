{
  try {
    log.info("Publisher closed");
    this.PUBLISHER.close();
    this.SESSION.close();
    this.CONN.close();
    this.PUBLISHER=null;
    this.SESSION=null;
    this.CONN=null;
  }
 catch (  JMSException e) {
    log.error(e.getMessage());
  }
catch (  Throwable e) {
    log.error(e.getMessage());
  }
}
