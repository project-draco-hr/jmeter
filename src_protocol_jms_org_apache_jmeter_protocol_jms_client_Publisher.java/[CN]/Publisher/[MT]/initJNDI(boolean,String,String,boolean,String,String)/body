{
  if (useProps) {
    try {
      return new InitialContext();
    }
 catch (    NamingException e) {
      log.error(e.getMessage());
      return null;
    }
  }
 else {
    return InitialContextFactory.lookupContext(initialContextFactory,providerUrl,useAuth,securityPrincipal,securityCredentials);
  }
}
