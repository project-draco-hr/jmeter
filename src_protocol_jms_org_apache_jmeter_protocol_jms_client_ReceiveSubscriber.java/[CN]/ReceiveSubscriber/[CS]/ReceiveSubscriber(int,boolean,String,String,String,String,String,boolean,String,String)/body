{
  Context ctx=InitialContextFactory.getContext(useProps,initialContextFactory,providerUrl,useAuth,securityPrincipal,securityCredentials);
  CONN=Utils.getConnection(ctx,connfactory);
  SESSION=CONN.createSession(false,Session.AUTO_ACKNOWLEDGE);
  Destination dest=Utils.lookupDestination(ctx,destinationName);
  SUBSCRIBER=createSubscriber(SESSION,dest,durableSubscriptionId);
  if (queueSize <= 0) {
    queue=new LinkedBlockingQueue<Message>();
  }
 else {
    queue=new LinkedBlockingQueue<Message>(queueSize);
  }
  SUBSCRIBER.setMessageListener(this);
  log.debug("<init> complete");
}
