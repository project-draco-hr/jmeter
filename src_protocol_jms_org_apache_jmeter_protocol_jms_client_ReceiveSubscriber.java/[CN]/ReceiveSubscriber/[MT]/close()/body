{
  this.RUN=false;
  try {
    this.CONN.stop();
    Utils.close(SUBSCRIBER,log);
    Utils.close(SESSION,log);
    Utils.close(CONN,log);
    this.CLIENTTHREAD.interrupt();
    this.CLIENTTHREAD=null;
    queue.clear();
  }
 catch (  JMSException e) {
    log.error(e.getMessage());
  }
catch (  Exception e) {
    log.error(e.getMessage());
  }
}
