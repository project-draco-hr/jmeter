{
  PlaceHolder holder=table.get(id);
  if (log.isDebugEnabled()) {
    log.debug("RPL_ID [" + id + "] for holder "+ holder);
  }
  if (holder != null) {
    holder.setReply(reply);
    Object obj=holder.getRequest();
synchronized (obj) {
      obj.notify();
    }
  }
 else {
    if (log.isDebugEnabled()) {
      log.debug("Failed to match reply: " + reply);
    }
  }
}
