{
  publisher=new Publisher(getUseJNDIPropertiesAsBoolean(),getJNDIInitialContextFactory(),getProviderUrl(),getConnectionFactory(),getDestination(),isUseAuth(),getUsername(),getPassword(),isDestinationStatic(),getUseNonPersistentDelivery());
  ClientPool.addClient(publisher);
  log.debug("PublisherSampler.initClient called");
}
