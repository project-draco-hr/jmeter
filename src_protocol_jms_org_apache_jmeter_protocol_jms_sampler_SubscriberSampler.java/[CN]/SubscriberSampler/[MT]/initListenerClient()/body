{
  interrupted=false;
  OnMessageSubscriber sub=(OnMessageSubscriber)ClientPool.get(this);
  if (sub == null) {
    sub=new OnMessageSubscriber(this.getUseJNDIPropertiesAsBoolean(),this.getJNDIInitialContextFactory(),this.getProviderUrl(),this.getConnectionFactory(),this.getTopic(),this.isUseAuth(),this.getUsername(),this.getPassword());
    sub.setMessageListener(this);
    sub.resume();
    ClientPool.addClient(sub);
    ClientPool.put(this,sub);
    log.debug("SubscriberSampler.initListenerClient called");
    log.debug("loop count " + this.getIterations());
  }
  return sub;
}
