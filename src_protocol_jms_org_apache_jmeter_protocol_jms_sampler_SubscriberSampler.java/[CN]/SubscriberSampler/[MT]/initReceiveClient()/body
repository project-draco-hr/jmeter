{
  interrupted=false;
  this.SUBSCRIBER=new ReceiveSubscriber(this.getUseJNDIPropertiesAsBoolean(),this.getJNDIInitialContextFactory(),this.getProviderUrl(),this.getConnectionFactory(),this.getTopic(),this.isUseAuth(),this.getUsername(),this.getPassword());
  this.SUBSCRIBER.resume();
  ClientPool.addClient(this.SUBSCRIBER);
  log.debug("SubscriberSampler.initReceiveClient called");
}
