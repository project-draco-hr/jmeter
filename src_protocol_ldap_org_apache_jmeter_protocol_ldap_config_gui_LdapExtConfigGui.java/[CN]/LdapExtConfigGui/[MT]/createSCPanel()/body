{
  VerticalPanel scPanel=new VerticalPanel();
  scPanel.add(scope);
  scPanel.add(createLabelPanel("countlim",countlim));
  scPanel.add(createLabelPanel("timelim",timelim));
  scPanel.add(createLabelPanel("attrs",attribs));
  scPanel.add(retobj);
  scPanel.add(deref);
  scPanel.add(parseflag);
  return scPanel;
}
