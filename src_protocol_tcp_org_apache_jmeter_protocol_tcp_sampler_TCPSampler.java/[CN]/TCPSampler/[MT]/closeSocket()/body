{
  Map cp=(Map)tp.get();
  if (hashKey == null)   return;
  Socket con=(Socket)cp.remove(hashKey);
  if (con != null) {
    log.debug(getName() + " Closing socket for " + hashKey);
    try {
      con.close();
    }
 catch (    IOException e) {
      log.warn(getName() + " Error closing socket for " + hashKey+ " "+ e);
    }
  }
}
