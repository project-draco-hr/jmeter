{
  ArrayList checked=new ArrayList();
  if (this.parent != null) {
    for (int idx=0; idx < AbstractTable.items.length; idx++) {
      if (this.parent.getPropertyAsString(AbstractTable.items[idx]).equals(String.valueOf(true))) {
        checked.add(AbstractTable.items[idx]);
      }
    }
  }
  return checked;
}
