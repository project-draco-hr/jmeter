{
  sample.setResponseData("value=\"${ID}\" Group$ctl00$drpEmails",null);
  assertion.unsetNotType();
  assertion.setToContainsType();
  assertion.setTestFieldResponseData();
  assertion.addTestString("value=\"\\${ID}\" Group\\$ctl00\\$drpEmails");
