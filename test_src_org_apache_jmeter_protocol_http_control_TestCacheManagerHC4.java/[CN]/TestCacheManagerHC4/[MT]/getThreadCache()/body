{
  Field threadLocalfield=CacheManager.class.getDeclaredField("threadCache");
  threadLocalfield.setAccessible(true);
  @SuppressWarnings("unchecked") ThreadLocal<Map<String,CacheEntry>> threadLocal=(ThreadLocal<Map<String,CacheManager.CacheEntry>>)threadLocalfield.get(this.cacheManager);
  return threadLocal.get();
}
