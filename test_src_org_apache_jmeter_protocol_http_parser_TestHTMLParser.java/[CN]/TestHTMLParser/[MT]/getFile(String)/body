{
  ArrayList<String> al=new ArrayList<String>();
  if (file != null && file.length() > 0) {
    BufferedReader br=new BufferedReader(new FileReader(findTestFile(file)));
    String line=br.readLine();
    while (line != null) {
      al.add(line);
      line=br.readLine();
    }
    br.close();
  }
  return al;
}
