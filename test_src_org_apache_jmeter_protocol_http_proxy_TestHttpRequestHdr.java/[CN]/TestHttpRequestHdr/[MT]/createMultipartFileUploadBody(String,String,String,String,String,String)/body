{
  String postBody="--" + boundary + endOfLine+ "Content-Disposition: form-data; name=\""+ fileField+ "\" filename=\""+ fileName+ "\""+ endOfLine+ "Content-Type: "+ fileMimeType+ endOfLine+ "Content-Transfer-Encoding: binary"+ endOfLine+ endOfLine+ fileContent+ endOfLine+ "--"+ boundary+ "--"+ endOfLine;
  return postBody;
}
