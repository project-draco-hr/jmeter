{
  testSanitizeUrl("http://localhost/","http://localhost/");
  testSanitizeUrl("http://localhost/a/b/c%7Cd","http://localhost/a/b/c|d");
  testSanitizeUrl("http://localhost:8080/%5B%5D","http://localhost:8080/%5B%5D");
  testSanitizeUrl("http://localhost:8080/?%5B%5D","http://localhost:8080/?%5B%5D");
  testSanitizeUrl("http://localhost:8080/?!??$*():@~;'%22%25%5E%7B%7D[]%3C%3E%7C%5C#","http://localhost:8080/?!??$*():@~;'\"%^{}[]<>|\\#");
  testSanitizeUrl("http://localhost:8080/?!??$*():@~;'%22%25%5E%7B%7D[]%3C%3E%7C%5C#","http://localhost:8080/?!??$*():@~;'%22%25%5E%7B%7D[]%3C%3E%7C%5C#");
  testSanitizeUrl("http://localhost:8080/!??$*():@~;'%22%25%5E%7B%7D%5B%5D%3C%3E%7C%5C#","http://localhost:8080/!??$*():@~;'\"%^{}[]<>|\\#");
  testSanitizeUrl("http://localhost:8080/!??$*():@~;'%22%25%5E%7B%7D%5B%5D%3C%3E%7C%5C#","http://localhost:8080/!??$*():@~;'%22%25%5E%7B%7D%5B%5D%3C%3E%7C%5C#");
}
