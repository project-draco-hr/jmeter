{
  TestSuite ts=new TestSuite("Resources PackageTest");
  ts.addTest(new PackageTest("atestDefault"));
  String lang[]=JMeterMenuBar.getLanguages();
  for (int i=0; i < lang.length; i++) {
    if (!"en".equals(lang[i])) {
      ts.addTest(new PackageTest("testLang",lang[i]));
    }
  }
  return ts;
}
