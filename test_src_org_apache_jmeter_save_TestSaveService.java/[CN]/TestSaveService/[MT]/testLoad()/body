{
  for (int i=0; i < FILES_LOAD_ONLY.length; i++) {
    InputStream in=null;
    try {
      in=new FileInputStream(findTestFile("testfiles/" + FILES_LOAD_ONLY[i]));
      HashTree tree=SaveService.loadTree(in);
      assertNotNull(tree);
    }
  finally {
      if (in != null) {
        in.close();
      }
    }
  }
}
