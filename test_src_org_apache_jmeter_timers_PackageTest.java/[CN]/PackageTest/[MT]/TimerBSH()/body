{
  if (!BeanShellInterpreter.isInterpreterPresent()) {
    final String msg="BeanShell jar not present, test ignored";
    log.warn(msg);
    return;
  }
  BeanShellTimer timer=new BeanShellTimer();
  long delay;
  timer.setScript("\"60\"");
  delay=timer.delay();
  assertEquals(60,delay);
  timer.setScript("60");
  delay=timer.delay();
  assertEquals(60,delay);
  timer.setScript("5*3*4");
  delay=timer.delay();
  assertEquals(60,delay);
}
