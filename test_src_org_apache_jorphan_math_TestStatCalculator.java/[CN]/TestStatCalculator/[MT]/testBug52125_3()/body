{
  calc.addValue(1L);
  calc.addValue(2L);
  calc.addValue(3L);
  StatCalculatorLong calc2=new StatCalculatorLong();
  calc2.addValue(2L);
  calc2.addValue(2L);
  calc2.addValue(2L);
  calc.addAll(calc2);
  assertEquals(6,calc.getCount());
  assertEquals(12.0,calc.getSum());
  assertEquals(0.5773502691896255,calc.getStandardDeviation());
}
