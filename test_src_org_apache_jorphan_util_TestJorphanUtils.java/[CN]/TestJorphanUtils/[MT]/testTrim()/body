{
  assertEquals("",JOrphanUtils.trim(""," ;"));
  assertEquals("",JOrphanUtils.trim(" "," ;"));
  assertEquals("",JOrphanUtils.trim("; "," ;"));
  assertEquals("",JOrphanUtils.trim(";;"," ;"));
  assertEquals("",JOrphanUtils.trim("  "," ;"));
  assertEquals("abc",JOrphanUtils.trim("abc ;"," ;"));
}
